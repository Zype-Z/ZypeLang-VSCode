{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "Zype",
    "patterns": [{
            "include": "#keywords"
        },
        {
            "include": "#strings"
        }
    ],
    "repository": {
        "strings": {
            "name": "string.quoted.double.zype",
            "begin": "\"",
            "end": "\"",
            "patterns": [{
                "name": "constant.character.escape.zype",
                "match": "\\\\."
            }]
        },
        "keys": {
            "name": "key.zype",
            "begin": "<",
            "end": ">"
        }
    },
    "scopeName": "source.zype"
}